      },
      "dotnetRunMessages": "true",
      "applicationUrl": "https://localhost:8001;http://localhost:9001"
    },
    "Scode.WebApi Production": {
      "commandName": "Project",
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Production",
        "API-KEY": "demokey"
      },
      "dotnetRunMessages": "true",
      "applicationUrl": "https://localhost:8001;http://localhost:9001"
    },
    "Docker": {
      "commandName": "Docker",
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/swagger",
      "publishAllPorts": true,
      "useSSL": true
    }
