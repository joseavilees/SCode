    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:55604",
      "sslPort": 44345
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "Scode.WebApi Development": {
      "commandName": "Project",
      "launchUrl": "swagger",
