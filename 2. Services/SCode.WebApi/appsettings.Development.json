{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Debug"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [{Level:u3}] *{Origin}* [{SourceContext}] {Message}{NewLine}{Exception}"
        }
      }
    ]
  },
  "StudentWebAppUrl": "http://localhost:4200"
}
